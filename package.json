{
  "name": "ts-init",
  "version": "0.1.0",
  "description": "Starting point for TypeScript projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "src/"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "coverage": "npm run build && npm run tap & tap --coverage-report=html",
    "debug": "cross-env DEBUG=* npm start",
    "lint": "npm run tslint && npm run eslint",
    "tslint": "tslint --fix src/**",
    "eslint": "eslint --fix .",
    "preversion": "npm test",
    "start": "node lib",
    "tap": "tap test -Rspec --cov",
    "test": "npm run tslint && npm run build && npm run eslint && npm run tap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanbtucker/ts-init.git"
  },
  "author": "Jordan Tucker <jordanbtucker@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordanbtucker/ts-init/issues"
  },
  "homepage": "https://github.com/jordanbtucker/ts-init#readme",
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^6.2.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/node": "^10.12.18",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.2",
    "tap": "^12.1.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  }
}
