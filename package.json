{
  "name": "ts-init",
  "version": "0.0.0",
  "description": "Starting point for TypeScript projects",
  "private": true,
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "engines": {
    "node": "^10.9 || ^12 || >=14"
  },
  "scripts": {
    "ava": "ava test/*",
    "build": "run-s clean compile",
    "clean": "rimraf lib",
    "compile": "tsc",
    "coverage": "run-s -c spec nyc-report",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "lint": "run-s eslint prettier",
    "nyc": "nyc npm run ava",
    "nyc-report": "nyc report --reporter=html",
    "prettier": "prettier --loglevel warn --write **/*.{babelrc,css,html,js,json,jsx,md,scss,ts,tsx,yml}",
    "preversion": "npm test",
    "spec": "run-s build nyc",
    "start": "node lib",
    "test": "run-s build lint nyc"
  },
  "dependencies": {
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "ava": "^3.15.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
