{
  "name": "ts-init",
  "version": "0.0.0",
  "description": "Starting point for TypeScript projects",
  "private": true,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf lib",
    "compile": "tsc",
    "coverage": "run-s -c spec tap-coverage",
    "eslint": "eslint --ext .js,.ts --fix .",
    "lint": "run-s eslint prettier",
    "prettier": "prettier --loglevel warn --write **/*.{js,json,md,ts,yml}",
    "preversion": "npm test",
    "spec": "run-s build tap",
    "start": "node lib",
    "tap": "tap test -R spec",
    "tap-coverage": "tap --coverage-report html",
    "test": "run-s build lint tap"
  },
  "dependencies": {
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0",
    "tap": "^14.7.1",
    "typescript": "^3.6.4"
  }
}
