{
  "name": "ts-init",
  "version": "0.0.0",
  "description": "A starting point for TypeScript projects",
  "private": true,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "./lib/**/*.{d.ts,js,js.map,json}",
    "./src/**/*.{js,json,ts}",
    "!./src/**/tsconfig.json"
  ],
  "engines": {
    "node": "^14 || ^16 || >=18",
    "npm": ">=7"
  },
  "scripts": {
    "build": "run-s clean tsc:build",
    "changelog": "git add CHANGELOG.md",
    "clean": "rimraf lib/*",
    "coverage": "run-s -c build tap tap:coverage",
    "eslint": "eslint --fix .",
    "lint": "run-s eslint prettier",
    "prepare": "husky install",
    "prettier": "prettier --loglevel warn -w .",
    "preversion": "run-s test changelog",
    "snapshot": "run-s lint build tap:snapshot",
    "tap": "tap -R spec --ts test",
    "tap:coverage": "tap --coverage-report html",
    "tap:snapshot": "tap --snapshot --ts test",
    "test": "run-s lint build tap",
    "tsc:build": "tsc --build",
    "tsc:watch": "tsc --build --watch",
    "watch": "run-s clean tsc:watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@tsconfig/node14": "^1.0.3",
    "@types/node": "^18.7.23",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
